      <geom name="ramp-15" pos="3.5 0 0.155" type="box" size="2.5 1 .5" euler="0 -15 0" rgba="0.6 0 0.6 1"/>
      <geom name="platform-15" pos="10.78 0 0.78" type="box" size="5 1 .5" euler="0 0 0" rgba="0.6 0 0.6 1"/>

      
    </worldbody>

    <include file="robot_3_dof.xml"/>

    <actuator>
        <motor name="FL_Abduction" joint="AbductionMotor_FL" gear="1" ctrllimited="true" ctrlrange="-10 10"/>
        <motor name="FL_Up" joint="HipMotor_FL" gear="1" ctrllimited="true" ctrlrange="-10 10"/>
        <motor name="FL_Down" joint="KneeMotor_FL" gear="1" ctrllimited="true" ctrlrange="-10 10"/>

        <motor name="FR_Abduction" joint="AbductionMotor_FR" gear="1" ctrllimited="true" ctrlrange="-10 10"/>
        <motor name="FR_Up" joint="HipMotor_FR" gear="1" ctrllimited="true" ctrlrange="-10 10"/>
        <motor name="FR_Down" joint="KneeMotor_FR" gear="1" ctrllimited="true" ctrlrange="-10 10"/>

        <motor name="RL_Abduction" joint="AbductionMotor_RL" gear="1" ctrllimited="true" ctrlrange="-10 10"/>
        <motor name="RL_Up" joint="HipMotor_RL" gear="1" ctrllimited="true" ctrlrange="-10 10"/>
        <motor name="RL_Down" joint="KneeMotor_RL" gear="1" ctrllimited="true" ctrlrange="-10 10"/>

        <motor name="RR_Abduction" joint="AbductionMotor_RR" gear="1" ctrllimited="true" ctrlrange="-10 10"/>
        <motor name="RR_Up" joint="HipMotor_RR" gear="1" ctrllimited="true" ctrlrange="-10 10"/>
        <motor name="RR_Down" joint="KneeMotor_RR" gear="1" ctrllimited="true" ctrlrange="-10 10"/>
    </actuator>

    <sensor>
        <user name="Upright" dim="3" user="6 1 0 3 0.05" />
        <user name="Height" dim="1" user="6 1 0 3 0.04" />
        <user name="Position" dim="3" user="2 0.2 0 3 0.1" />
        <user name="Gait" dim="4" user="6 2 0 10 0.03" />
        <user name="Balance" dim="2" user="2 0.2 0 0.3 0.1" />
        <user name="Effort" dim="12" user="0 0.06 0.0 0.1" />
        <user name="Posture" dim="12" user="0 0.02 0.0 0.1" />
        <user name="Orientation" dim="2" user="0 0.1 0 2" />
        <user name="Angmom" dim="3" user="0 0 0 .03" />
        <user name="Consistency" dim="1" user="6 0.1 0 1 0.05"/>

        <framepos name="trace0" objtype="site" objname="head" />
        <subtreecom name="torso_subtreecom" body="main_body" />
        <subtreelinvel name="torso_subtreelinvel" body="main_body"/>
        <subtreeangmom name="torso_angmom" body="main_body"/>
        <framequat     name="orientation" objtype="body" objname="main_body"/>
    </sensor>

